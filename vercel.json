{
  "version": 2,
  "functions": {
    "api/php/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  }, 
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/fetch_reservations", "destination": "/api/fetch_reservations.php" }
  ],
  "cleanUrls": true, 
  "trailingSlash": false  
}