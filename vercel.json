{
  "functions": {
    "api/pages/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/profile", "destination": "/api/profile.php" },
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
