{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  }, 
  "rewrites": [
    {
      "source": "/fetch_reservations",
      "destination": "/api/fetch_reservations.php"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "cleanUrls": true, 
  "trailingSlash": false  
}
